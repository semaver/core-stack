# Set default behaviour, in case users don't have core.autocrlf set.
* text=auto

*.ts eol=lf
*.as eol=lf
*.css eol=lf
*.h eol=lf
*.iml eol=lf
*.ini eol=lf
*.java eol=lf
*.js eol=lf
*.json eol=lf
*.jsonp eol=lf
*.m eol=lf
*.mxml eol=lf
*.txt eol=lf
*.pch eol=lf
*.php eol=lf
*.properties eol=lf
*.rb eol=lf
*.sh eol=lf
*.xml eol=lf

# Unity files
*.unity eol=lf
*.prefab eol=lf
*.meta eol=lf
*.cs eol=lf
*.asset eol=lf

# exact file names
.project eol=lf
.gitignore eol=lf

# Text files which need to be treated as binary because they may
# contain UTF-16 data (e.g. Adobe Flash files)
fla/**/*.xml binary

# Denote all files that are truly binary and should not be modified.
*.gif binary
*.gpg binary
*.jar binary
*.jpg binary
*.pdf binary
*.png binary
*.swc binary
*.swf binary
*.xfl binary
