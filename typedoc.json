{
  "extends": [
    "./typedoc.base.json"
  ],
  "entryPointStrategy": "packages",
  "entryPoints": [
    "packages/*"
  ],
  "out": "docs",
  "exclude": [
    "**/lib/**/*",
    "**/tests/**/*",
    "*.js",
    "**/*.d.ts",
    "**/node_modules/**/*"
  ],
  "name": "Semaver Libraries",
  "readme": "README.md",
  "theme": "default",
  "includeVersion": true,
  "hideGenerator": true,
  "excludePrivate": true,
  "gitRevision": "master"
}
